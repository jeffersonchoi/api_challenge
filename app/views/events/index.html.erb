<table>
  <tr>
    <th>id</th>
    <th>name</th>
    <th>longitude</th>
    <th>latitude</th>
    <th>description</th>
  </tr>

    <% @events.each do |event| %>
  <tr>
    <td><%= event.id %></td>
    <td><%= truncate(event.name) %></td>
    <td><%= event.longitude %></td>
    <td><%= event.latitude %></td>
    <td><%= truncate(event.description) %></td>
  </tr>
  <% end %>

</table>
